<div class="galerie">
  <% for (let [index, imag] of locals.imagini.entries()){ %> <% if (index <= 9)
  { %>
  <figure class="<%= `fig-${index}` %>" title="<%= imag.descriere %>">
    <picture>
      <source srcset="<%- imag.mic %>" media="(max-width: 750px)" />
      <source srcset="<%- imag.mediu %>" media="(max-width: 1600px)" />
      <img
        src="<%- imag.mare %>"
        alt="<%- imag.alt ? imag.alt : imag.descriere  %>"
      />
    </picture>
    <figcaption>
      <%- imag.titlu ? imag.titlu : "Masina" %> <%- imag.autor ? ", de " +
      imag.autor : "" %> <%- imag.license ? " sub licenta " + imag.license : ""
      %>
    </figcaption>
  </figure>
  <% } %> <% } %>
</div>
